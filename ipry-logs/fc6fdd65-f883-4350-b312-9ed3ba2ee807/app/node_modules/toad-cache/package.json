{
  "name": "toad-cache",
  "description": "LRU and FIFO caches for Client or Server",
  "version": "3.7.0",
  "homepage": "https://github.com/kibertoad/toad-cache",
  "author": "Igor Savin <kibertoad@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/toad-cache.git"
  },
  "files": [
    "dist",
    "*.d.ts",
    "*.d.cts"
  ],
  "license": "MIT",
  "source": "index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./toad-cache.d.ts",
